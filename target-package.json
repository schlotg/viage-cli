{
  "name": "viage-app",
  "version": "1.0.0",
  "description": "Modern, minimal, fast web framework",
  "main": "dist/index.html",
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "start": "webpack-dev-server --open --config webpack.dev.js",
    "build-embedded": "webpack --config webpack.prod.js && node embed",
    "serve": "node server",
    "test": "jest"
  },
  "author": "Unknown",
  "license": "None",
  "devDependencies": {
    "@types/jest": "^23.3.10",
    "clean-webpack-plugin": "^1.0.0",
    "css-loader": "^2.0.0",
    "express": "^4.16.4",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.23.1",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.1",
    "typescript": "^3.2.2",
    "url-loader": "^1.1.2",
    "viage": "^1.3.14",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.4"
  },
  "jest": {
    "transform": {
      "^.+(viage).+js": "ts-jest",
      "^.+\\.jsx?$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
